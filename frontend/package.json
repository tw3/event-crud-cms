{
	"name": "event-crud-cms-frontend",
	"version": "1.0.0",
	"description": "A front-end web application to Create, Read, Update and Delete a list of scheduled events.",
	"keywords": [
		"react",
		"redux"
	],
	"main": "index.js",
	"homepage": "https://github.com/tw3/event-crud-cms/",
	"repository": {
		"type": "git",
		"url": "https://github.com/tw3/event-crud-cms/"
	},
	"scripts": {
		"start": "webpack-dev-server --inline --hot --history-api-fallback --host 0.0.0.0 --port 8080",
		"build-dev": "webpack --config webpack.dev.config.js",
		"build-prod": "webpack -p --config webpack.prod.config.js",
		"test": "mocha --recursive --compilers js:babel-register --require babel-polyfill --require ignore-styles",
		"test-watch": "npm test -- --watch"
	},
	"babel": {
		"presets": [
			"es2015",
			"react",
			"stage-3"
		],
		"plugins": [
			"react-hot-loader/babel"
		]
	},
	"author": "Ted Weatherly",
	"license": "MIT",
	"engines": {
		"node": "6.9.2"
	},
	"dependencies": {
		"axios": "^0.15.3",
		"js-logger": "^1.3.0",
		"moment": "^2.17.1",
		"react": "^15.4.1",
		"react-bootstrap": "^0.30.7",
		"react-datetime": "^2.8.1",
		"react-dom": "^15.4.1",
		"react-redux": "^5.0.1",
		"react-router": "^3.0.0",
		"react-router-bootstrap": "^0.23.1",
		"react-router-redux": "^4.0.7",
		"react-widgets": "^3.4.5",
		"redux": "^3.6.0",
		"redux-form": "^6.4.3",
		"redux-logger": "^2.7.4",
		"redux-saga": "^0.14.0",
		"validate.js": "^0.11.1"
	},
	"devDependencies": {
		"babel-core": "^6.21.0",
		"babel-eslint": "^7.1.1",
		"babel-loader": "^6.2.10",
		"babel-polyfill": "^6.20.0",
		"babel-preset-es2015": "^6.18.0",
		"babel-preset-react": "^6.16.0",
		"babel-preset-stage-3": "^6.17.0",
		"babel-runtime": "^6.20.0",
		"clean-webpack-plugin": "^0.1.14",
		"css-loader": "^0.26.1",
		"enzyme": "^2.7.0",
		"eslint": "^2.13.1",
		"eslint-config-airbnb": "^9.0.1",
		"eslint-loader": "^1.4.0",
		"eslint-plugin-import": "^1.10.2",
		"eslint-plugin-jsx-a11y": "^1.5.5",
		"eslint-plugin-react": "^5.2.2",
		"extract-text-webpack-plugin": "^1.0.1",
		"file-loader": "^0.9.0",
		"ignore-styles": "^5.0.1",
		"less": "^2.7.1",
		"less-loader": "^2.2.3",
		"mocha": "^3.2.0",
		"node-sass": "^4.1.1",
		"react-addons-test-utils": "^15.4.1",
		"react-hot-loader": "^3.0.0-beta.6",
		"redux-freeze": "^0.1.5",
		"sass-loader": "^4.1.1",
		"style-loader": "^0.13.1",
		"url-loader": "^0.5.7",
		"webpack": "^1.14.0",
		"webpack-dev-server": "^1.16.2",
		"whatwg-fetch": "^2.0.1"
	}
}
